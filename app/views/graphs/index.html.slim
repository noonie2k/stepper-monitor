#graph

javascript:
  var chart1; // globally available
  $(document).ready(function() {
    chart1 = new Highcharts.Chart({
      chart: {
        renderTo: 'graph',
        zoomType: 'x'
      },
      title: {
        text: 'Step Count'
      },
      xAxis: {
        type: 'datetime',
        maxZoom: 7 * 24 * 60 * 60 * 1000,
        dateTimeLabelFormats: {
          month: '%e. %b',
          year: '%b'
        }
      },
      yAxis: {
          title: {
          text: 'Steps taken'
        }
      },
      series: [#{@seriesJS}]
    });
  });
